<!--pages/index/equipment/detail/detail.wxml-->
<view class="detail">
	<view class="yuan"></view>
	<view class="main">
		<image bindtap="preview" src="../../../../images/banner.png"></image>
		<view class="lists">
			<view class="li">
				
				<view>
					<text>设备名称:</text>
					<text>{{item.cszzlxname}}</text>
				</view>
				<view>
					<text>设备型号:</text>
					<text>{{item.cszzxh}}</text>
				</view>
				<view>
					<text>投用日期:</text>
					<text>{{item.trsyrq || '暂无'}}</text>
				</view>
				<view>
					<text>使用年限:</text>
					<text>{{item.serviceLife || '暂无'}}</text>
				</view>
				<view>
					<text>安装人:</text>
					<text>{{item.installers || '暂无'}}</text>
				</view>
				<view>
					<text>生产商:</text>
					<text>{{item.sccs || '暂无'}}</text>
				</view>
				 
			</view>
		</view>
	</view>
</view>
