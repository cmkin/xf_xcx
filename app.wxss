.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}

.clearfix {
	zoom: 1;
}

.omg{
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}


